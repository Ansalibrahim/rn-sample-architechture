{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": ["react", "import", "jsx-a11y", "react-native"],
  "rules": {
    "linebreak-style": 0,
    "react/jsx-filename-extension": [
      "error",
      {
        "extensions": [".js", ".jsx"]
      }
    ]
  },
  "globals": {
    "fetch": true,
    "__DEV__": true
  }
}
